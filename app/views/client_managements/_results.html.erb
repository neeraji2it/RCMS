<table width="100%">
  <% @clients.each do |client| %>
    <tr>
      <td style="background-color: #fff;color:#000;"><%= client.first_name %></td>
      <td style="background-color: #fff;color:#000;"><%= link_to "add client",add_client_client_management_path(current_user.role,client,:appointment_id => @appointment.id),:remote => true %></td>
    </tr>
  <% end %>
</table>
